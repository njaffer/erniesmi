<div class="container">

  <h4> Import Stamps Club</h4> 
 
  <%= form_tag(import_about_index_path, :enctype =>"multipart/form-data") do %>

  <div class="row">
     <div class="field">
       <%#= label_tag :file %>
       <%#= file_field_tag :file,  :required => true  %>
     </div>
   </div>

  <div class="card" style="width: 45rem; padding-left: 10px;">  
  <p>
  <%= file_field_tag :file  %>
  </p>
  </div>
  
  <div class="form-group" style="padding-top: 20px">
    <div class="col-sm-offset-2 col-sm-10">
      <button type="submit" class="btn btn-primary">Save</button> 
      <%#<button type="Reset" class="btn btn-secondary">Reset Form</button> %>
    </div>
  </div>

  <% end %>
</div>